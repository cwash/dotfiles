



# eclipse's Ant runtime only looks in here for 
# its env. variables I think...
export JAVA_HOME=/System/Library/Frameworks/JavaVM.framework/Versions/1.6/Home
export M2_HOME=/Java/apache-maven-2.0.9
export AXIS2_HOME=/Java/axis2-1.3
export M2=$M2_HOME/bin
export PATH=$M2:$PATH

alias jb='sudo $JBOSS_HOME/bin/run.sh -b 0.0.0.0'
alias sjb='sudo $JBOSS_HOME/bin/shutdown.sh -S'
alias ls='ls -CFG'
alias ll='ls -la'

test -r /sw/bin/init.sh && . /sw/bin/init.sh

